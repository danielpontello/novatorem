                   right: 0;
                }

                .progressBar {
                   width: 85px;
                   height: 3px;
                   background: red;
                   position: relative;
                   background-color: #1a1a1a;
                   margin-top: .5px;
                   border-radius:2px;
                }

                .fill {
                   position: absolute;
                   top: 0;
                   left: 0;
                   background: #1DB954;
                   width: 50%;
                   height: 3px;
                   border-radius:2px;
                   {% if now_playing %}
                   animation-name: bar;
                   animation-duration: {{animation_time}};
                   animation-timing-function: linear;
                   animation-fill-mode: forwards;
